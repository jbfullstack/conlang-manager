{
  "name": "conlang-manager",
  "version": "1.0.0",
  "description": "Personnal language manager",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:start": "docker-compose up -d postgres redis",
    "db:stop": "docker-compose down",
    "db:reset": "docker-compose down -v && docker-compose up -d postgres redis && timeout /t 5 /nobreak && npm run db:push",
    "db:logs": "docker-compose logs -f postgres",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "setup": "npm install && npm run db:start && timeout /t 5 /nobreak && npm run db:push && npm run db:seed",
    "test": "jest",
    "postinstall": "prisma generate",
    "vercel-build": "prisma generate && prisma db push && next build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbfullstack/conlang-manager.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jbfullstack/conlang-manager/issues"
  },
  "homepage": "https://github.com/jbfullstack/conlang-manager#readme",
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.13.0",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.5",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^15.4.5",
    "next-auth": "^4.24.11",
    "openai": "^5.12.0",
    "prisma": "^6.13.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "swr": "^2.3.6",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "autoprefixer": "^10.4.21",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.0.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.3"
  }
}
